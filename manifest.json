{
  "name": "Canonical",
  "description": "If a page has a canonical rel link, provide a quick way to navigate to it.",
  "version": "1.0",
  "permissions": [ "tabs", "http://*/*" ],
  "background_page": "background.html",
  "icons": {
    "128": "logo128.png"
  },
  "page_action": {
    "default_icon": "logo16.png"
  },
  "content_scripts": [ {
    "js": [ "jquery-1.4.2.js", "sniff_common.js" ],
    "matches": [ "http://*/*" ],
    "run_at": "document_end"
  } ]
}
